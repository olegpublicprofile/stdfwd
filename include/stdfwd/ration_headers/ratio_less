#pragma once

#include "../stdfwd_details.hpp"

//------------------------------------------------------------------------------

STDFWD_STD_BEGIN

template <class R1, class R2> struct ratio_less;

STDFWD_STD_END

//------------------------------------------------------------------------------

namespace stdfwd
{
	template <class R1, class R2>
	using ratio_less = std::ratio_less<R1, R2>;

} // namespace stdfwd

//------------------------------------------------------------------------------
