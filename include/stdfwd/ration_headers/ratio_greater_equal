#pragma once

#include "../stdfwd_details.hpp"

//------------------------------------------------------------------------------

STDFWD_STD_BEGIN

	template <class R1, class R2> struct ratio_greater_equal;

STDFWD_STD_END

//------------------------------------------------------------------------------

namespace stdfwd {

	template <class R1, class R2>
	using ratio_greater_equal = std::ratio_greater_equal<R1, R2>;

} // namespace stdfwd

