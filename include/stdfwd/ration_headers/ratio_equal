#pragma once

#include "../stdfwd_details.hpp"

//------------------------------------------------------------------------------

STDFWD_STD_BEGIN

	template <class R1, class R2> struct ratio_equal;

STDFWD_STD_END

//------------------------------------------------------------------------------

namespace stdfwd
{

	template <class R1, class R2>
	using ratio_equal = std::ratio_equal<R1, R2>;
}

